<div class="container">
	<% @users.each do |user| %>
		<span><%= link_to user_path(user.id) do %>
	              	<%= attachment_image_tag user, :image, :fill, 80, 80, fallback: "No_image.png",size: '80x80', class:"user-image" %>
	              <% end %>
	        </span>
	   		<p><%= link_to user.nickname, user_path(user.id), class:"user-name" %></p>
	<% end %>
</div>